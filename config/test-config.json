{
    "azureLocation": {
        "description": "the location for your Azure Datacenter",
        "shellscript": "",
        "default": "uswest3",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
    "keyCloakClientSecret": {
        "description": "the secret the service needs to be a KeyCloak 'client'",
        "shellscript": "",
        "default": "",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
    "keyCloakHost": {
        "description": "the host (with port and prefix) for the Key Cloak Service",
        "shellscript": "",
        "default": "http://localhost:8080",
        "tempEnvironmentVariableName": "KEYCLOAK_HOST",
        "requiredAsEnv": true
    },
    "keyCloakRealm": {
        "description": "the KeyCloak realm",
        "shellscript": "",
        "default": "keycloak-realm",
        "tempEnvironmentVariableName": "KEYCLOAK_REALM",
        "requiredAsEnv": true
    },
    "keyCloakClientId": {
        "description": "the id (name) of the KeyCloak client",
        "shellscript": "",
        "default": "test-client",
        "tempEnvironmentVariableName": "KEYCLOAK_CLIENT_ID",
        "requiredAsEnv": true
    },
    "keyCloakAdminUserName": {
        "description": "the login name for the KeyCloak admin",
        "shellscript": "",
        "default": "admin",
        "tempEnvironmentVariableName": "KEYCLOAK_ADMIN_USERNAME",
        "requiredAsEnv": true
    },
    "keyCloakAdminPassword": {
        "description": "the password for the KeyCloak admin account",
        "shellscript": "",
        "default": "",
        "tempEnvironmentVariableName": "KEYCLOAK_ADMIN_PASSWORD",
        "requiredAsEnv": true
    },
    "keyCloakTestPassword": {
        "description": "the password for the KeyCloak test client(s)",
        "shellscript": "",
        "default": "",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
    "keyCloakResourceGroup": {
        "description": "the Resource Group to install Key Cloak into",
        "shellscript": "",
        "default": "KEYCLOAK-RG",
        "tempEnvironmentVariableName": "KEYCLOAK_RESOURCE_GROUP",
        "requiredAsEnv": true
    },
    "keyCloakAksName": {
        "description": "the name of the Key Cloak AKS Service",
        "shellscript": "",
        "default": "KEYCLOAK-AKS",
        "tempEnvironmentVariableName": "KEYCLOAK_AKS",
        "requiredAsEnv": true
    }
}